<!DOCTYPE html>
<html>
<head>
    <partial name="PartialViews/_head"/>
</head>
<body>
    <header>
        <partial name="PartialViews/_header" />
    </header>

    <div class="container page-content sidebar-page right-sidebar-page clearfix" style="margin-top: 100px;">
        <div class="page-content-wrapper">
            <div class="page-content-inner">

                @RenderBody()

            </div>
        </div>
    </div>


    <footer class="border-top footer text-muted">
        <partial name="PartialViews/_footer" />
    </footer>

    <partial name="PartialViews/_scripts" />

    @RenderSection("Scripts", required: false)
</body>
</html>
